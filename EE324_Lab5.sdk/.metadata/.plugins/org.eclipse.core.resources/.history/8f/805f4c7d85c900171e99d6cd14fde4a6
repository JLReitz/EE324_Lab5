#include "xparameters.h"
#include "xil_types.h"


#define VGA_BaseAddr	0x43C00000
#define VGA_H_Sync		0x43C00000
#define VGA_H_BP		0x43C00000
#define VGA_H_FP		0x43C00000
#define VGA_H_Range		0x43C00000
#define VGA_H_LR		0x43C00000
#define VGA_V_Sync		0x43C00000
#define VGA_V_BP		0x43C00000
#define VGA_V_FP		0x43C00000
#define VGA_V_Range		0x43C00000
#define VGA_V_TB		0x43C00000
#define VGA_Color		0x43C00000


void writeValue(u32 * addr, u32 value)
{
	*addr = value;
}

void main()
{

}
